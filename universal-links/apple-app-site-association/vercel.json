{
  "rewrites": [
    { "source": "/ul", "destination": "/api/ul" },
    { "source": "/ul/(.*)", "destination": "/api/ul?path=$1" }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        }
      ]
    }
  ]
}
